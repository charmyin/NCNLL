  <body>
   

<nav class="navbar navbar-default" role="navigation" style="margin-bottom: 0px;">
    <div class="container-fluid">
        <!-- Brand and toggle get grouped for better mobile display -->
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navbarHead">
                <span class="sr-only"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="/#!/">NCNEL<span style="font-size:10px;">(beta)</span></a>
        </div>

        <!-- Collect the nav links, forms, and other content for toggling -->
        <div class="collapse navbar-collapse" ng-controller="headCategoryCtrl" id="navbarHead">
            <ul class="nav navbar-nav">
                <li class="dropdown" id="syfl">
                    <a style="cursor:pointer" class="dropdown-toggle" data-toggle="dropdown"><span class="glyphicon glyphicon-th"></span>&nbsp;&nbsp;摄影分类<b class="caret"></b></a>
                    <ul class="dropdown-menu">
                        <li><a href="#!/category"><span class="glyphicon glyphicon-th"></span>&nbsp;&nbsp;所有分类</a></li>
                        <li class="divider"></li>
                        <li ng-repeat="category in categories | orderBy:'categoryOrderIndex'"><a href="#!/product/category/{{category._id}}"><span class="{{category.iconName}}"></span>&nbsp;&nbsp;{{category.categoryName}}</a></li>
                    </ul>
                </li>
 
                <li id="photographers"><a href="#!/producers"><span class="glyphicon glyphicon-facetime-video"></span>&nbsp;制作者</a></li>

                <!-- <li id="photographers"><a href="/photographers"><span class="glyphicon glyphicon-facetime-video"></span>&nbsp;摄影师</a></li>
                <li id="sysb"><a href="/cameras"><span class="glyphicon glyphicon-camera"></span>&nbsp;摄影设备</a></li> -->
            </ul>

            <form class="navbar-form navbar-left" role="search">
                <div class="form-group">
                    <input type="text" class="form-control" placeholder="输入关键字...">
                </div>
                <button type="submit" class="btn btn-default">搜索</button>
            </form>
            <ul class="nav navbar-nav navbar-right" ng-controller="rightNavbarCtl">
                <li id="loginHead" ng-hide="loginSuccessfully"><a style="cursor:pointer;" data-toggle="modal" data-target="#loginModal"><span class="glyphicon glyphicon-user"></span>&nbsp;登录</a></li>
                 <li class="dropdown" ng-show="loginSuccessfully">
                    <a style="cursor:pointer" class="dropdown-toggle" data-toggle="dropdown"><span class="glyphicon glyphicon-user"></span>欢迎您<b class="caret"></b></a>
                    <ul class="dropdown-menu">
                        <li><a href="#!/producer/infoManage/userId">用户信息管理</a></li>

                        <li><a href="#!/producer/changePassword/userid">密码修改</a></li>
                        <li class="divider"></li>
                        <li><a style="cursor:pointer" ng-click="logout()">退出</a></li>
                    </ul>
                </li>
                <li class="dropdown" ng-show="loginSuccessfully">
                    <a style="cursor:pointer" class="dropdown-toggle" data-toggle="dropdown"><span class="glyphicon glyphicon-link"></span>我的菜单 <b class="caret"></b></a>
                    <ul class="dropdown-menu">
                        <li><a href="#!/products/storaged">我的收藏</a></li>
                    </ul>
                </li>
                <li class="dropdown">
                    <a style="cursor:pointer" class="dropdown-toggle" data-toggle="dropdown"><span class="glyphicon glyphicon-link"></span>友情链接 <b class="caret"></b></a>
                    <ul class="dropdown-menu">
                        <li><a href="http://www.wwoof.org/">WWOOF</a></li>
                        <li class="divider"></li>
                        <li><a href="http://www.brinno.com.cn/">BRINNO</a></li>
                    </ul>
                </li>
            </ul>
        </div><!-- /.navbar-collapse -->
    </div><!-- /.container-fluid -->
