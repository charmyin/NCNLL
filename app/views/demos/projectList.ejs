<ol class="breadcrumb ng-scope" style="margin:0;">
    <li><a href="/#!/">首页</a></li>
    <li class="active">生产项目</li>
</ol>


    <form id="formId" action="/demo/productList" method="post">
        <input type="hidden" name="search_eq_projectId" id="search_eq_projectId">
    </form>
    <!--Body-->
    <div class="row" style="margin:5px" id="bodyDiv">
        <% for(var i=0; i<dataList.length; i++) {%>
           <div class="col-sm-6 col-md-4"><a href="javascript:gotoProductList('<%= dataList[i]._id %>')" ><%= dataList[i].name %></a></div>
        <% } %>
    </div>

